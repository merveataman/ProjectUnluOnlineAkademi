@model List<WhyUsListDto>
<section class="ab_one section-padding">
    <div class="container">
        <div class="row">
            <div class="col-12 wow fadeInUp" data-wow-duration="1s" data-wow-delay="0.1s">
                <div class="ab_content">
                    <h2 style="color:#ff0000; text-decoration:underline;">Neden  Ünlü Online Akademi ?</h2>
                </div>
            </div>
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-4 col-sm-6 col-12 abmv wow fadeInUp"
                     data-wow-duration="1s" data-wow-delay="0.1s" data-wow-offset="0">
                    <span class="@item.Icon"></span>
                    <h5>@item.Title</h5>
                    <p>@item.Desc</p>
                </div>
            }
        </div>
    </div>
</section>